@model Tuple<NGL.Web.Models.ParentCourse.FindParentCourseModel, IEnumerable<NGL.Web.Models.ParentCourse.GradeModel>>

@{
    ViewBag.Title = "Assign Student Grades";
}

<h2>@ViewBag.Title</h2>

<h4><strong>SESSION</strong></h4>
@Html.TextBoxFor(m => m.Item1.Session, new { @class = "form-control full-input", @id = "Session" })
<h4><strong>SECTION</strong></h4>
@Html.TextBoxFor(m => m.Item1.Section, new { @class = "form-control full-input", @id = "Section" })

@Html.HiddenFor(m => m.Item1.SessionId, new { @id = "SessionId" })
@Html.HiddenFor(m => m.Item1.SectionId, new { @id = "SectionId"})
<br/>
<div class="panel panel-default">
    <h4><strong>STUDENTS</strong></h4>

</div>
@section Scripts {
    Ngl.shared.sessionAutocomplete.init();
    Ngl.shared.sectionAutocomplete.init();
    Ngl.report.getGrades.init();
}